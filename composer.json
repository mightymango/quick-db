{
    "name": "mightymango/quick-db",
    "description": "Artisan database commands",
    "keywords": [
        "MightyMango",
        "laravel",
        "quick_db"
    ],
    "homepage": "https://github.com/mightymango/quick_db",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Barrett",
            "email": "simon@mightymango.ltd",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4",
        "doctrine/dbal": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "Mightymango\\QuickDb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mightymango\\QuickDb\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mightymango\\QuickDb\\QuickDbServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
